{
  "surah_number": 114,
  "ayahs": [
    {
      "ayah_number": 1,
      "translation_tl": "Sabihin mo: “Nagpapakupkop ako sa Panginoon ng mga tao,",
      "footnotes": null
    },
    {
      "ayah_number": 2,
      "translation_tl": "na Hari ng mga tao,",
      "footnotes": null
    },
    {
      "ayah_number": 3,
      "translation_tl": "na Diyos ng mga tao,",
      "footnotes": null
    },
    {
      "ayah_number": 4,
      "translation_tl": "laban sa kasamaan ng tagapasaring na palaurong,",
      "footnotes": null
    },
    {
      "ayah_number": 5,
      "translation_tl": "na nagpapasaring [ng kasamaan] sa mga dibdib ng mga tao,",
      "footnotes": null
    },
    {
      "ayah_number": 6,
      "translation_tl": "kabilang sa mga jinn at mga tao.”",
      "footnotes": null
    }
  ]
}